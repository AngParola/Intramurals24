          "_name": "custom-html",
          "_sourceTheme": "mobirise5",
          "_customHTML": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Grouped Table by Sport with Male and Female Rows</title>\r\n\r\n    <link rel=\"stylesheet\" href=\"https://cdn.datatables.net/2.1.6/css/dataTables.dataTables.css\">\r\n    <link rel=\"stylesheet\" href=\"https://cdn.datatables.net/buttons/3.1.2/css/buttons.dataTables.css\">\r\n\r\n    <style>\r\n        /* Style for table columns to enhance distinction */\r\n        #example th, #example td {\r\n            border: 2px solid #000000; /* Light border around each cell */\r\n            padding: 10px;\r\n            text-align: center;\r\n            font-weight: bold;\r\n            vertical-align: middle;\r\n            width: 15%; /* Set a uniform width for each column */\r\n        }\r\n\r\n        /* Distinct background colors for each column */\r\n        #example tbody tr td:nth-child(1),\r\n        #example tbody tr td:nth-child(2) ,\r\n        #example tbody tr td:nth-child(3), \r\n        #example tbody tr td:nth-child(4), \r\n        #example tbody tr td:nth-child(5), \r\n        #example tbody tr td:nth-child(6) {\r\n            background-color: #f9f9f9; /* Optional: background color for distinction */\r\n        }\r\n\r\n        /* Styling for header */\r\n        #example thead th {\r\n            background-color: #ffffff;\r\n            color: rgb(0, 0, 0);\r\n        }\r\n\r\n        /* Centering header images and setting a standard size */\r\n        #example thead th img {\r\n            display: block;\r\n            margin: 0 auto;\r\n            width: 40%; /* Adjust image size if necessary */\r\n        }\r\n    </style>\r\n\r\n</head>\r\n<body>\r\n<div style=\"width: 95%; margin:0 auto; display: grid;\r\ngrid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\noverflow-x: scroll;\">\r\n    <table id=\"example\" class=\"display nowrap\" style=\"width:100%\">\r\n        <thead>\r\n            <tr>\r\n                <th>Sport</th>\r\n                <th>Gender</th>\r\n                <th><center>Rank (Score)</center></th>\r\n\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            <!-- Basketball group -->\r\n            <tr>\r\n                <td><center>Basketball</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Basketball</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            \r\n            <!-- Sepak Takraw group -->\r\n            <tr>\r\n                <td><center>Sepak Takraw</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Sepak Takraw</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Volleyball group -->\r\n            <tr>\r\n                <td><center>Volleyball</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Volleyball</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Tug of War group -->\r\n            <tr>\r\n                <td><center>Tug of War</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Tug of War</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Pop Dance group -->\r\n            <tr>\r\n                <td><center>Pop Dance</center></td>\r\n                <td><center>Mix Male and Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Bench Cheer group -->\r\n            <tr>\r\n                <td><center>Bench Cheer</center></td>\r\n                <td><center> </center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Best in Yell group -->\r\n            <tr>\r\n                <td><center>Best in Yell</center></td>\r\n                <td><center> </center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            \r\n            <!-- Most Disciplined District group -->\r\n            <tr>\r\n                <td><center>Most Disciplined District</center></td>\r\n                <td><center> </center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Badminton group -->\r\n            <tr>\r\n                <td><center>Badminton</center></td>\r\n                <td><center>Doubles</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Table Tennis group -->\r\n            <tr>\r\n                <td><center>Table Tennis</center></td>\r\n                <td><center>Singles</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Futsal group -->\r\n            <tr>\r\n                <td><center>Futsal</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>1</center></td>\r\n\r\n                \r\n            </tr>\r\n\r\n            <!-- Sack Race group -->\r\n            <tr>\r\n                <td><center>Sack Race</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Sack Race</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>0</center></td>\r\n\r\n                \r\n            </tr>\r\n            \r\n            <!-- Chess group -->\r\n            <tr>\r\n                <td><center>Chess (Board 1)</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center>2nd (1)</center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Chess (Board 1)</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center>2nd (1)</center></td>\r\n\r\n                \r\n            </tr>\r\n            \r\n            <!-- Chess group -->\r\n            <tr>\r\n                <td><center>Chess (Board 2)</center></td>\r\n                <td><center>Male</center></td>\r\n                <td><center> </center></td>\r\n\r\n                \r\n            </tr>\r\n            <tr>\r\n                <td><center>Chess (Board 2)</center></td>\r\n                <td><center>Female</center></td>\r\n                <td><center> </center></td>\r\n\r\n                \r\n            </tr>\r\n            \r\n\r\n        </tbody>\r\n    </table>\r\n</div>\r\n\r\n<script src=\"https://code.jquery.com/jquery-3.7.1.js\"></script>\r\n<script src=\"https://cdn.datatables.net/2.1.6/js/dataTables.js\"></script>\r\n<script src=\"https://cdn.datatables.net/buttons/3.1.2/js/dataTables.buttons.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jszip/3.10.1/jszip.min.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.2.7/pdfmake.min.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.2.7/vfs_fonts.js\"></script>\r\n<script src=\"https://cdn.datatables.net/buttons/3.1.2/js/buttons.html5.min.js\"></script>\r\n<script src=\"https://cdn.datatables.net/buttons/3.1.2/js/buttons.print.min.js\"></script>\r\n\r\n<script>\r\n    $(document).ready(function() {\r\n        var table = $('#example').DataTable({\r\n            \"columnDefs\": [\r\n                { \"visible\": false, \"targets\": 0 } // Hide the 'Sport' column for visual clarity\r\n            ],\r\n            \"order\": [[0, 'asc'], [1, 'asc']], // Order by Sport first, then Gender\r\n            \"pageLength\": -1, // Show all rows\r\n            \"drawCallback\": function(settings) {\r\n                var api = this.api();\r\n                var rows = api.rows({ page: 'current' }).nodes();\r\n                var lastSport = null;\r\n\r\n                api.column(0, { page: 'current' }).data().each(function(sport, i) {\r\n                    if (lastSport !== sport) {\r\n                        $(rows).eq(i).before(\r\n                            '<tr class=\"group\"><td colspan=\"6\"><strong>' + sport + '</strong></td></tr>'\r\n                        );\r\n                        lastSport = sport;\r\n                    }\r\n                });\r\n            }\r\n        });\r\n\r\n        // Order by the grouping\r\n        $('#example tbody').on('click', 'tr.group', function() {\r\n            var currentOrder = table.order()[0];\r\n            if (currentOrder[0] === 0 && currentOrder[1] === 'asc') {\r\n                table.order([0, 'desc']).draw();\r\n            } else {\r\n                table.order([0, 'asc']).draw();\r\n            }\r\n        });\r\n    });\r\n</script>\r\n</body>\r\n</html>\r\n",
